{
    "builds": [{
        "src": "InnovateITSchool/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "InnovateITSchool/wsgi.py"
        }
    ]
}
{
    "version": 2,
    "builds": [
      {
        "src": "InnovateITSchool/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ], 
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "InnovateITSchool/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
    }